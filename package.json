{
  "name": "composer-system-tests",
  "version": "0.0.0",
  "dependencies": {
    "async": "^0.9.0",
    "chalk": "^1.1.1",
    "fs-extra": "^0.24.0",
    "grunt": "~0.4.5",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-protractor-runner": "~3.2.0",
    "grunt-protractor-webdriver": "~0.2.5",
    "jshint-stylish": "^1.0.0",
    "load-grunt-tasks": "~3.1.0",
    "lodash": "^2.4.1",
    "log4js": "^0.6.21",
    "protractor": "~3.3.0",
    "chromedriver": "latest",
    "eyes.protractor": "0.0.38",
    "grunt-contrib-jshint": "^0.10.0",
    "grunt-mocha-test": "^0.12.7",
    "jasmine-reporters": "~1.0.2"
  },
  "scripts": {
    "chromedriver:version": "sed -i.bak 's/\"chromedriver\": \"[0-9\\.]*\"/\"chromedriver\": \"2.25\"/' node_modules/protractor/config.json",
    "selenium:version": "sed -i.bak 's/\"selenium\": \"[0-9\\.]*\"/\"selenium\": \"2.53.1\"/' node_modules/protractor/config.json",
    "webdriver:update": "node ./node_modules/protractor/bin/webdriver-manager update --standalone",
    "postinstall": "npm run chromedriver:version && npm run selenium:version && npm run webdriver:update"
  }
}
